    <!-- the controller passes ‘telebirr_vals_json’ inside rendering_values -->
    <div class="o_telebirr_inline_form" name="o_telebirr_inline_container">
      <script type="application/json" name="telebirr_data">
        <t t-out="telebirr_vals_json or '{}'"/>
