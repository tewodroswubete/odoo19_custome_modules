<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="payment_provider_telebirr" model="payment.provider">
            <field name="name">Telebirr</field>
            <field name="code">telebirr</field>
            <field name="state">disabled</field>
            <field name="inline_form_view_id" ref="payment_telebirr.telebirr_inline_form"/>
            <field name="image_128" type="base64" file="payment_telebirr/static/description/logo.png"/>
            <!-- link the wallet payment-method -->
            <field name="payment_method_ids" eval="[(4, ref('payment_method_telebirr'))]"/>
        </record>
    </data>
</odoo>